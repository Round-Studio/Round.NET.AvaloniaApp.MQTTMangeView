<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Round.NET.AvaloniaApp.MQTTMangeView.Views.Pages.AddData.AddData">
    <Grid>
        <StackPanel>
            <DockPanel Margin="5">
                <Label VerticalAlignment="Center" Width="90">收取主题：</Label>  
                <ComboBox HorizontalAlignment="Stretch" Name="TopicSelBox"></ComboBox>
            </DockPanel>
            <DockPanel Margin="5">
                <Label VerticalAlignment="Center" Width="90">备注文字：</Label>  
                <TextBox HorizontalAlignment="Stretch" Name="NoteBox" Watermark="备注文字...">暂无介绍...</TextBox>
            </DockPanel>
        </StackPanel>
    </Grid>
</UserControl>
